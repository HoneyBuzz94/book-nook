<body>
  <h1>{{username}}'s Nook</h1>
  <button type="button" id="logout" class="btn btn-outline-secondary">Logout</button>
  <a class="btn btn-primary" href="/journal" role="button">Create new entry</a>
  {{#each books as |book|}}
    <div class="row align-center mb-5">
      <div class="col-md-6">
        <h2>{{book.title}} by {{book.author}}</h2>
        <p>{{book.time_stamp}}</p>
      </div>
      <div class="col-md-6">
        <p>{{book.synopsis}}</p>
        <p>{{book.review}}</p>
      </div>
    </div>
    <button id="deleteBTN" type="button" data-id={{book.id}}  onclick="handleNoteDelete(event)" class="btn-delete">delete</button>
  {{/each}}

</body>

<script src="/js/logout.js"></script>